{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "URLs": "http://localhost:5010;https://localhost:5011",
  "HTTPS_PORT": "5011",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    },
    "Certificates": {
      "Default": {
        "Path": "client.pfx",
        "Password": "client_password"
      }
    }
  },
  "Microservices": {
    "ServiceAPI": "https://localhost:5021/api/",
    "CertPath":  "service.crt"
  }
}
